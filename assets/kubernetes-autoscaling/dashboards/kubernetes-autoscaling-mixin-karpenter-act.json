         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "round(\n  sum(\n    increase(\n      karpenter_nodeclaims_disrupted_total{\n        job=~\"$job\",\n        nodepool=~\"$nodepool\"\n      }[$__rate_interval]\n    )\n  ) by (nodepool, capacity_type, reason)\n)\n",
               "interval": "1m",
               "legendFormat": "{{ nodepool }} - {{ capacity_type }} - {{ reason }}"
