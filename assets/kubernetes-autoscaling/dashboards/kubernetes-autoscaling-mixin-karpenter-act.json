         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "round(\n  sum(\n    increase(\n      karpenter_voluntary_disruption_eligible_nodes{\n        job=~\"$job\",\n      }[$__rate_interval]\n    )\n  ) by (reason)\n)\n",
               "interval": "1m",
               "legendFormat": "{{ reason }}"
            }
