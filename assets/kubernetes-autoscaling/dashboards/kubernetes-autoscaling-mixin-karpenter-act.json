   "templating": {
      "list": [
         {
            "label": "Data source",
            "name": "datasource",
            "query": "prometheus",
            "type": "datasource"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${datasource}"
            },
            "label": "Job",
            "name": "job",
            "query": "label_values(karpenter_nodes_allocatable{}, job)",
            "refresh": 2,
            "sort": 1,
            "type": "query"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${datasource}"
            },
            "includeAll": true,
            "label": "Node Pool",
            "multi": true,
            "name": "nodepool",
            "query": "label_values(karpenter_nodepools_allowed_disruptions{job=~\"$job\"}, nodepool)",
            "refresh": 2,
            "sort": 1,
            "type": "query"
         }
      ]
   },
