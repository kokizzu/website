   "description": "A dashboard that monitors Django which focuses on breaking down requests by view. The dashboards were generated using [Django-mixin](https://github.com/adinhodovic/django-mixin). Open issues and create feature requests in the repository.",
   "editable": true,
   "links": [
      {
         "asDropdown": true,
         "includeVars": true,
         "keepTime": true,
         "tags": [
            "django",
            "django-mixin"
         ],
         "targetBlank": true,
         "title": "Django",
         "type": "dashboards"
      }
   ],
   "panels": [
      {
         "collapsed": false,
         "gridPos": {
            "h": 1,
            "w": 24,
            "x": 0,
            "y": 0
         },
         "id": 1,
         "title": "Summary [1h]",
         "type": "row"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Traffic split by Django view over the last hour.",
         "fieldConfig": {
            "defaults": {
               "unit": "reqps"
            },
            "overrides": [ ]
         },
         "gridPos": {
            "h": 5,
            "w": 8,
            "x": 0,
            "y": 1
         },
         "id": 2,
         "options": {
            "displayLabels": [
               "percent"
            ],
            "legend": {
               "displayMode": "table",
               "placement": "right",
               "showLegend": true,
               "values": [
                  "percent"
               ]
            },
            "tooltip": {
               "mode": "multi",
               "sort": "desc"
            }
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum(\n  rate(\n    django_http_requests_total_by_view_transport_method_total{\n      cluster=\"$cluster\",\nnamespace=\"$namespace\",\njob=~\"$job\"\n,\nview!~\"<unnamed view>|health_check:health_check_home|prometheus-django-metrics\"\n,\n      method=~\"$method\"\n    }[1h]\n  )\n) by (view)\n",
               "instant": true,
               "legendFormat": "{{ view }}"
            }
         ],
         "title": "Request Distribution by View [1h]",
         "type": "piechart"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "Traffic split by HTTP method for the selected view over the last hour.",
